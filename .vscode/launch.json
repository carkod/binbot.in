{
  "version": "0.0.2",
  "configurations": [
    {
      "type": "chrome",
      "request": "launch",
      "name": "React: Web",
      "url": "http://localhost:3000",
      "webRoot": "${workspaceFolder}/web/src",
      "preLaunchTask": "startApp",
      "postDebugTask": "stopApp",
      "sourceMapPathOverrides": {
        "webpack:///src/*": "${webRoot}/*"
      }
    }
  ]
}
